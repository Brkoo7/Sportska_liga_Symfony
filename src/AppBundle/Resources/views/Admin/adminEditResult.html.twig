{% extends 'AppBundle:Admin:adminResults.html.twig' %}

{% block sidebar %}
	<ul class="list-group col-md-5  aside">
	    <p id="message"></p>
	    <h2>Uređivanje rezultata: {{result.homeTeam.name}} - {{result.awayTeam.name}}
	    	| {{result.date_ |date('Y-m-d')}} </h2>
	    {{ form_start(form) }}
	    {{ form_row(form.homeGoals) }}
	    {{ form_row(form.awayGoals) }}
	    {{ form_row(form._token) }}
	    <button type="submit" class="btn btn-sm btn-danger update" formnovalidate>Ažuriraj</button>    
	</ul>


{% endblock sidebar %}

{% block javascripts %}
   {{parent()}}
    <script src="{{ asset('js/vendors/bootstrap-datepicker.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/formsEdit.js') }}"></script>
{% endblock %}