{% extends 'AppBundle:Admin:adminSchedules.html.twig' %}


{% block sidebar %}

<ul class="list-group col-md-5  aside">
    <p id="message"></p>
    <h2>Uređivanje rasporeda: {{schedule.homeTeam.name}} - {{schedule.awayTeam.name}} </h2>
    {{ form_start(form) }}
    {{ form_row(form.date_) }}
    {{ form_row(form._token) }}
    <button type="submit" class="btn btn-sm btn-danger update" formnovalidate>Ažuriraj</button>
</ul>

{% endblock sidebar %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/vendors/bootstrap-datepicker.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/formsEdit.js') }}"></script>
{% endblock %}
                
           

